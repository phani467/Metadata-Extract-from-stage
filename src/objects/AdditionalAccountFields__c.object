    <validationRules>
        <fullName>OneAdditionalAccountFieldForAccount</fullName>
        <active>false</active>
        <description>SFDC1-2077 This validation rule restricts an account to have only one additional account field record. Multiple additional account field records cannot be created for same account.</description>
        <errorConditionFormula>AND(
  ISNEW(), 
  NOT(ISBLANK(AccountReference__r.AdditionalAccountFields__c))
)</errorConditionFormula>
