    <fields>
        <fullName>CalculatedSortOrder__c</fullName>
        <description>SFDC1-7557, Formula to calculate the complexity of the rule</description>
        <externalId>false</externalId>
        <formula>If(ISBLANK(Subject__c),0,1)+
If(ISBLANK(Type__c),0,1)+
If(ISBLANK(Subtype__c),0,1)+
If(ISBLANK(WebEmail__c),0,1)+
If(ISBLANK(SkillLevel__c),0,1)+
If(ISBLANK( ProductFamily__c ),0,1)+
If(ISBLANK( ProductGroup__c ),0,1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Rule Complexity</inlineHelpText>
        <label>Calculated Sort Order</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
