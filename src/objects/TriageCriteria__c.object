    <validationRules>
        <fullName>PriorityMandatoryForTriageRule</fullName>
        <active>true</active>
        <description>SFDC1-7557 This rule mandates user to add priority of triage rule.</description>
        <errorConditionFormula>ISBLANK(Priority__c)</errorConditionFormula>
        <errorDisplayField>Priority__c</errorDisplayField>
        <errorMessage>Please specify priority of the rule.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductGroupMandatoryForIHSTriageRule</fullName>
        <active>false</active>
        <description>SFDC1-7557 This rule mandates user to add product group for IHS triage rule</description>
        <errorConditionFormula>AND(
ISBLANK(ProductGroup__c),
RecordType.DeveloperName == &apos;IHS&apos;
)</errorConditionFormula>
        <errorDisplayField>ProductGroup__c</errorDisplayField>
        <errorMessage>Please specify Product group for the rule.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QueueMandatoryForTriageRule</fullName>
        <active>true</active>
        <description>SFDC1-7557 This rule mandates user to add queue for triage rule.</description>
        <errorConditionFormula>ISBLANK(TEXT(Queue__c))</errorConditionFormula>
        <errorDisplayField>Queue__c</errorDisplayField>
        <errorMessage>Please specify Queue for the rule. In case a match is found, case will be assigned to this queue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubjectMandatoryForMSERVTriageRule</fullName>
        <active>false</active>
        <description>SFDC1-7557 This rule mandates user to add subject for MSERV triage rule</description>
        <errorConditionFormula>AND(
ISBLANK(Subject__c),
RecordType.DeveloperName == &apos;MarkitSERV&apos;
)</errorConditionFormula>
        <errorDisplayField>Subject__c</errorDisplayField>
        <errorMessage>Please specify Subject for the rule.</errorMessage>
    </validationRules>
