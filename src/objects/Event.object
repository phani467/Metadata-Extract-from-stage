    <validationRules>
        <fullName>RestrictEventCreateandEditbyProfiles</fullName>
        <active>true</active>
        <description>SFDC1-4739 : Restrict some Profiles from Event creation and edit.</description>
        <errorConditionFormula>AND( 
OR( 
ISNEW(), 
ISCHANGED(Subject), 
ISCHANGED(Location), 
ISCHANGED(IsAllDayEvent), 
ISCHANGED(Type), 
ISCHANGED(Description), 
ISCHANGED(TimeSpentMinutes__c), 
ISCHANGED(WhoId), 
ISCHANGED(WhatId), 
ISCHANGED(OwnerId) 
), 
OR( 
CONTAINS($Profile.Name, &apos;Read Only Profile&apos;), 
CONTAINS($Profile.Name, &apos;Data Governance&apos;), 
CONTAINS($Profile.Name, &apos;Sales: Product Manager&apos;), 
CONTAINS($Profile.Name, &apos;Sales: Marketing&apos;) 
),
LEFT(WhatId, 3) == &apos;800&apos;
)</errorConditionFormula>
        <errorMessage>You are not authorized to create or edit an Event.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>CreateMeetingNoteEvent</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Meeting Note</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)} 
if(&quot;{!Event.IsChild}&quot; == true) { 
alert(&quot;This is a shared meeting that is automatically generated. You can only create meeting notes on the original meeting event.&quot;); 
} else { 
if(&quot;{!Event.MeetingNoteId__c}&quot; == &quot;&quot;) { 

// determine object type 
var whatId = &quot;{!Event.What}&quot;; 
var prefix = whatId.substring(0,3); 

// both empty 
if((&quot;{!Event.Account__c}&quot; == null || &quot;{!Event.Account__c}&quot; == &quot;&quot;) &amp;&amp; (&quot;{!Event.What}&quot; == null || &quot;{!Event.What}&quot; == &quot;&quot;)) { 
alert(&quot;Please link this event to an Account or Opportunity.&quot;); 
} 
// Account field filled, related to not 
else if((&quot;{!Event.Account__c}&quot; != null &amp;&amp; &quot;{!Event.Account__c}&quot; != &quot;&quot;) &amp;&amp; (&quot;{!Event.What}&quot; == null || &quot;{!Event.What}&quot; == &quot;&quot; || prefix != &quot;006&quot;)) { 
var url = &quot;/apex/CreateMeetingNoteEvent?infoType=accOnly&amp;accId={!Event.AccountId__c}&amp;EventId={!Event.Id}&quot;; 
window.location.href = url; 
} 
// Account field empty but related to filled 
else if((&quot;{!Event.Account__c}&quot; == null || &quot;{!Event.Account__c}&quot; == &quot;&quot;) &amp;&amp; (&quot;{!Event.What}&quot; != null &amp;&amp; &quot;{!Event.What}&quot; != &quot;&quot;)) { 

// account 
if(prefix == &quot;001&quot;) { 
var url = &quot;/apex/CreateMeetingNoteEvent?infoType=accOnly&amp;accId={!Event.What}&amp;EventId={!Event.Id}&quot;; 
window.location.href = url; 
// opportunity 
} else if (prefix == &quot;006&quot;) { 
var url = &quot;/apex/CreateMeetingNoteEvent?infoType=oppOnly&amp;oppId={!Event.What}&amp;EventId={!Event.Id}&quot;; 
window.location.href = url; 
// something else, no meeting note atm 
} else { 
alert(&quot;Please link this event to an Account or Opportunity.&quot;); 
} 

// both filled 
} else if((&quot;{!Event.Account__c}&quot; != null &amp;&amp; &quot;{!Event.Account__c}&quot; != &quot;&quot;) &amp;&amp; (&quot;{!Event.What}&quot; != null &amp;&amp; &quot;{!Event.What}&quot; != &quot;&quot;)) { 
// determine object type 
var whatId = &quot;{!Event.What}&quot;; 
var prefix = whatId.substring(0,3); 

// account 
if(prefix == &quot;001&quot;) { 
var url = &quot;/apex/CreateMeetingNoteEvent?infoType=accOnly&amp;accId={!Event.What}&amp;EventId={!Event.Id}&quot;; 
window.location.href = url; 
// opportunity 
} else if (prefix == &quot;006&quot;) { 
var url = &quot;/apex/CreateMeetingNoteEvent?infoType=bothFilled&amp;accId={!Event.AccountId__c}&amp;oppId={!Event.What}&amp;EventId={!Event.Id}&quot;; 
window.location.href = url; 
// something else, no meeting note atm 
} else { 
alert(&quot;Please link this event to an Account or Opportunity.&quot;); 
} 
} else { 
alert(&quot;Please link this event to an Account or Opportunity.&quot;); 
} 
} else { 
alert(&quot;This event is already linked to a meeting note.&quot;); 
} 
}</url>
