    <validationRules>
        <fullName>AdClaimApproverCanNOTCreateAdClaimChange</fullName>
        <active>true</active>
        <description>SFDC1-1423 : Ad Claim Approver can not create AdClaim Change Log Record.</description>
        <errorConditionFormula>ISNEW() &amp;&amp; $User.AdClaimsApprover__c</errorConditionFormula>
        <errorMessage>You are not authorized to create Ad Claim Change Log record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AdClaimChangeCanNOTRejectAfterApprove</fullName>
        <active>true</active>
        <description>SFDC1-2023 : AdClaim Change can not be Rejected once Approved.</description>
        <errorConditionFormula>TEXT(Status__c) = &apos;Rejected&apos; &amp;&amp; TEXT(PRIORVALUE(Status__c)) = &apos;Approved&apos;</errorConditionFormula>
        <errorMessage>Approved AdClaim Change Log can not be Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ApprovedAdClaimChangelogEditOnlyApprover</fullName>
        <active>true</active>
        <description>SFDC1-2108 : Approved AdClaim Change log can be only edited by Ad Claim ChangeLog Approver</description>
        <errorConditionFormula>NOT( ISNEW() ) &amp;&amp; TEXT( Status__c ) = &apos;Approved&apos; &amp;&amp; NOT( $User.AdClaimsApprover__c )</errorConditionFormula>
        <errorMessage>Approved AdClaims Changelog can be edited only by Approvers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ChangeLogCanNotCreateWithDraftAdClaim</fullName>
        <active>true</active>
        <description>SFDC1-2140 : ADClaim Change Log can not be created with Draft AdClaim Record.</description>
        <errorConditionFormula>TEXT(AdClaim__r.ClaimsStatus__c ) = &apos;Draft&apos; &amp;&amp;  ISNEW()</errorConditionFormula>
        <errorMessage>AdClaim Change Log can not be created with Draft AdClaim.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieldsMandatoryToSubmitOrApproveRecord</fullName>
        <active>true</active>
        <description>SFDC1-1057 At least one field in the &quot;Change Description&quot; section must be filled out before submitting or approving.</description>
        <errorConditionFormula>AND(
	OR(
		ISPICKVAL( Status__c , &apos;Submitted&apos;),
		ISPICKVAL( Status__c , &apos;Approved&apos;)
		),
	AND( 
		ISBLANK( NewMediaType__c ),
		ISBLANK( OtherMediaType__c ),
		ISBLANK( ProposedClaimLanguage__c ),
		ISBLANK( NewProposedDisclaimerLanguage__c ),
		ISBLANK( ApprovedClaimLanguage__c ),
		ISBLANK( NewApprovedDisclaimerLanguage__c ),
		ISBLANK( ApprovalComments__c ),
		ISBLANK( RejectionComments__c )
		)
	)</errorConditionFormula>
        <errorMessage>At least one field in the &quot;Change Description&quot; section must be filled out before submitting or approving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OnlyApproverCanApproveRejectTheRecord</fullName>
        <active>true</active>
        <description>SFDC1-460 Only a user with &apos;Ad Claim Approver&apos; checkbox checked should be able to Approve/ reject the record.
SFDC1-2108 : Updating the Validation Rule to bypass after Record is Rejected and allowing user to edit.</description>
        <errorConditionFormula>AND(
	ISCHANGED(Status__c),
	OR(
		ISPICKVAL( Status__c , &apos;Rejected&apos;),
		ISPICKVAL( Status__c , &apos;Approved&apos;)
	),
	$User.AdClaimsApprover__c == FALSE
)</errorConditionFormula>
        <errorMessage>Only an Authorized Ad Claim Approver can Approve/ Reject the record. Please contact your System Administrator for more information.</errorMessage>
    </validationRules>
</CustomObject>
