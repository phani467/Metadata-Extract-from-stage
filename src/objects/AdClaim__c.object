        <errorConditionFormula>ISPICKVAL(ClaimsStatus__c, &apos;Approved&apos;) &amp;&amp;
ISPICKVAL(AdditionalValidationRequired__c, &apos;Yes&apos;) &amp;&amp;
ISBLANK(AdditionalValidationAssignedTo__c)</errorConditionFormula>
