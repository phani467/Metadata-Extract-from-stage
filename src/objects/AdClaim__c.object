        <errorConditionFormula>ISNEW() &amp;&amp;  $User.AdClaimsApprover__c</errorConditionFormula>
        <errorMessage>You are not authorized to create Ad Claim record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AdClaimCanNOTRejectAfterApprove</fullName>
        <active>true</active>
        <description>SFDC1-2023 : AdClaim can not be Rejected once Approved.</description>
        <errorConditionFormula>TEXT(ClaimsStatus__c ) = &apos;Rejected&apos; &amp;&amp;  TEXT(PRIORVALUE(ClaimsStatus__c )) = &apos;Approved&apos;</errorConditionFormula>
        <errorMessage>Approved AdClaim can not be Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ApprovedAdClaimCanBeEditedOnlyByApprover</fullName>
        <active>true</active>
        <description>SFDC1-2063 : Approved AdClaim can be only edited by Ad Claim Approver</description>
        <errorConditionFormula>NOT( ISNEW() ) &amp;&amp; TEXT( ClaimsStatus__c ) = &apos;Approved&apos; &amp;&amp; NOT( $User.AdClaimsApprover__c )</errorConditionFormula>
        <errorMessage>Approved AdClaims can be edited only by Approvers.</errorMessage>
    </validationRules>
