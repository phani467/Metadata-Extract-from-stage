<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>SFDC1-7059, SFDC1-7060 custom setting to hold field values that are not locked</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>APIName__c</fullName>
        <description>SFDC1-7059, SFDC1-7060 api name field to hold API name of unlocked field as the name field can only be 38 characters long</description>
        <externalId>false</externalId>
        <inlineHelpText>API name of the unlocked field</inlineHelpText>
        <label>API Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RuleName__c</fullName>
        <description>SFDC1-7059, SFDC1-7060 name of the validation rule to which the unlocked field is related to</description>
        <externalId>false</externalId>
        <inlineHelpText>Name of the validation rule to which the unlocked field is related to</inlineHelpText>
        <label>Rule Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Opportunity Validation Fields</label>
    <visibility>Public</visibility>
</CustomObject>
