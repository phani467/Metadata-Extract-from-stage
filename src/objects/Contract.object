    <sharingModel>Read</sharingModel>
    <validationRules>
        <fullName>ActivityCheckOnContractStatusRejected</fullName>
        <active>true</active>
        <description>SFDC1-6066 ,SFDC1-5673 : Activity is required when contract status is rejected.This validation will only be fired when user skip the stage 4 and activity check is skipped on opportunity.</description>
        <errorConditionFormula>AND(
TEXT(ContractStatus__c) = &quot;Rejected&quot;,
NOT(ISBLANK(Opportunity__c)),
TEXT(Opportunity__r.StageName) = &quot;5-Contracting&quot;, 
Opportunity__r.RecordType.Name = &quot;New Business&quot;, 
NOT(Opportunity__r.ActivityCreated__c) 
)</errorConditionFormula>
        <errorMessage>Associated opportunity must have an activity referenced to it before you reject the contract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AllApprovalsRequired</fullName>
        <active>true</active>
        <description>SFDC1-6282: All Approvers listed as Internal Approvers need to have their status set to &apos;Approved&apos; in order to move Contract Stage to &quot;Generation.&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(Status, &quot;Generation&quot;), ISPICKVAL(PRIORVALUE(Status), &quot;InternalApprovals&quot;), NOT( NumApprovalsApproved__c = NumApprovalRequested__c))</errorConditionFormula>
        <errorMessage>All Approvers listed as Internal Approvers need to have their status set to &apos;Approved&apos; in order to move Contract Stage to &quot;Generation.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AllowOnlyContractOwnerChangeForSG</fullName>
        <active>false</active>
        <description>SFDC1-4739 : Allow Only Contract Owner Change For Specific profile</description>
        <errorConditionFormula>AND(
NOT(ISCHANGED(OwnerId)), 
CONTAINS($Profile.Name, &apos;General&apos;),
$User.Id&lt;&gt;OwnerId 
)</errorConditionFormula>
        <errorMessage>You are not authorized to edit Contracts owned by others.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ApprovalRequired</fullName>
        <active>true</active>
        <description>SFDC1-6282:An Internal Approval must be requested in order to move the Contract Stage to Internal Approvals.</description>
        <errorConditionFormula>AND(ISPICKVAL(Status, &quot;InternalApprovals&quot;), (NumApprovalRequested__c = 0 ))</errorConditionFormula>
        <errorMessage>An Internal Approval must be requested in order to move the Contract Stage to Internal Approvals.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractEndDateFilter</fullName>
        <active>true</active>
        <description>SFDC1-5386: Contract End Date cannot be set to a date prior to Contract Start Date</description>
        <errorConditionFormula>(EndDate &lt; StartDate)</errorConditionFormula>
        <errorMessage>Contract End Date cannot be set to a date prior to Contract Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractedPriceIncreaseForOrderForm</fullName>
        <active>true</active>
        <description>SFDC1-5201 Contracted Price Increase required if &quot;Price Increase Clause&quot; is &quot;Notification - Contracted percent increase&quot; OR &quot;No Notification - Contracted percent increase&quot;</description>
        <errorConditionFormula>AND (
 RecordType.Name == &apos;Order Form&apos;,
 OR (
ISPICKVAL(PriceIncreaseClause__c,&apos;Notification - Contracted Percent Increase&apos;),
ISPICKVAL(PriceIncreaseClause__c,&apos;No Notification - Contracted Percent Increase&apos;)
),
ISBLANK( ContractedPriceIncrease__c )
)</errorConditionFormula>
        <errorDisplayField>ContractedPriceIncrease__c</errorDisplayField>
        <errorMessage>Contracted Price Increase&apos; is required for order form when &apos;Price Increase Clause&apos; is Notification - Contracted Percent Increase/ No Notification - Contracted Percent Increase</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CorporateCompliaceInternalApprovalReq</fullName>
        <active>true</active>
        <description>SFDC1-7774: Prevent a user from progressing on an Order Form without receiving Approval when needed for Corporate Compliance
SFDC1-8355 Update logic to use the new Opportunity&apos;s Compliance Restrictions field.
SFDC1-8822 Update</description>
        <errorConditionFormula>AND( 
    RecordType.Name == &apos;Order Form&apos;, 
    NOT(ISNEW()),
    OR( 
        ISPICKVAL(Status, &quot;Generation&quot;), 
        ISPICKVAL(Status, &quot;ReadyForProcessing&quot;)
    ), 
    NOT(ISPICKVAL(PRIORVALUE(Status), &quot;Creation&quot;)),    
    NumCorporateComplianceApprovalsApproved__c == 0,
    Opportunity__r.ComplianceRestrictions__c != &apos;&apos;
)</errorConditionFormula>
        <errorDisplayField>ContractStatus__c</errorDisplayField>
        <errorMessage>The customer associated with this contract has compliance restrictions and requires approval. Please create an Internal Approval and send to Corporate Compliance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DoNotAllowContractRecordTypeChange</fullName>
        <active>true</active>
        <description>SFDC1-5413 This rule does not allow anyone apart from System Administrator to change contract record type.</description>
        <errorConditionFormula>AND (
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;, 
ISCHANGED(RecordTypeId)
)</errorConditionFormula>
        <errorMessage>Contract Record type cannot be updated once record is created. Please contact your System Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DoNotAllowMarkTheStageAsComplete</fullName>
        <active>true</active>
        <description>SFDC1-6032 restrict Nonadmin users update stage complete. Added Ischanged so not fire unnecessarily.
SFDC1-7189 Updated &apos;General Agreement&apos; to &apos;Master Agreement&apos;
SFDC1-7855: can be set completed by OM
SFDC1-8872: CC and Delivery allowed also</description>
        <errorConditionFormula>AND ( RecordType.Name &lt;&gt; &quot;NDA&quot;,
RecordType.Name &lt;&gt; &quot;Master Agreement&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;,
NOT(CONTAINS($Profile.Name, &apos;Delivery&apos;)),
NOT(CONTAINS($Profile.Name, &apos;Customer Care&apos;)),
NOT(CONTAINS($Profile.Name, &apos;Order Management&apos;)),
ISCHANGED(Status), 
ISPICKVAL(Status, &quot;Complete&quot;) 
)</errorConditionFormula>
        <errorMessage>Contract can&apos;t be marked as Complete. Please contact your System Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EffectiveDateLessThanContractStartDate</fullName>
        <active>true</active>
        <description>SFDC1-5489 This validation ensures effective date is before or equal the Contract Start Date</description>
        <errorConditionFormula>AND( 
	OR ( 
	RecordType.Name = &apos;Order Form&apos;, 
	RecordType.Name = &apos;Trial&apos;,
	RecordType.Name = &apos;Continuation Letter&apos;
	),
	NOT(ISNEW()),
	EffectiveDate__c &gt;  StartDate,
	AND( 
		NOT(ISCHANGED( IHSMarkitLegalEntityStreet__c )), 
		NOT(ISCHANGED( BillToClientName__c )), 
		NOT(ISCHANGED( GlobalAllianceAddressCity__c)), 
		NOT(ISCHANGED( ShipToCity__c )),
		NOT(ISCHANGED( ClientNoticeAddressCity__c)),
		NOT(ISCHANGED( ClientLegalEntityCity__c))		
	) 
)</errorConditionFormula>
        <errorMessage>The Effective Date must be before or equal the Contract Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GeneralAgreementForFastrackOrderForm</fullName>
        <active>true</active>
        <description>SFDC1-5851 General Agreement Type and Agreement Effective Date is mandatory on Non Fastrack Order Forms.
SFDC1-6916 changes for opportunity record type &apos;Gratis&apos;, since it cannot have an amount.</description>
        <errorConditionFormula>AND(  
	RecordType.Name == &apos;Order Form&apos;, 
	NOT(ISNEW()),
	Fastrack__c == FALSE, 
        Opportunity__r.RecordType.Name&lt;&gt;&apos;Gratis&apos;,
        OR(
                TEXT(Status) == &apos;Generation&apos;,
                TEXT(Status) == &apos;ReadyforProcessing&apos;),
	ByPassValidationsForProcessBuilder__c == FALSE, 
	NOT(ISCHANGED(ByPassValidationsForProcessBuilder__c)),
	NOT(ISCHANGED(TotalContractValue__c)),
	NOT(ISCHANGED(FirstYearContractValue__c)),
	OR( 
		ISBLANK(TEXT(GeneralAgreementType__c)),
		AND (
				ISPICKVAL(GeneralAgreementType__c,&apos;Other&apos;)
				,
		ISBLANK(GeneralAgreementDate__c)
		)
	)	 
)</errorConditionFormula>
        <errorMessage>Master Agreement Type and Agreement Effective Date is mandatory on Non Fastrack Order Forms, please update the Contract accordingly to proceed further.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GeneralAgreementRequiredOnOrderForm</fullName>
        <active>true</active>
        <description>SFDC1-5851 General Agreement mandatory when General Agreement Type is &quot;IHS Markit General Agreement&quot; or &quot;Markit General Agreement&quot;
SFDC1-7189 Updating error for Master Agreement</description>
        <errorConditionFormula>AND (
  RecordType.Name == &apos;Order Form&apos;,
  OR (
  ISPICKVAL( GeneralAgreementType__c , &apos;IHS Markit Master Agreement&apos;),
  ISPICKVAL( GeneralAgreementType__c , &apos;IHS General Agreement&apos;),
  ISPICKVAL( GeneralAgreementType__c , &apos;IHS Master Agreement&apos;),
  ISPICKVAL( GeneralAgreementType__c , &apos;Markit Master Agreement&apos;)
  ),
  ISBLANK(GeneralAgreement__c) 
)</errorConditionFormula>
        <errorMessage>Master Agreement is required when Master Agreement Type is not &quot;Other&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GeneralAgreementValueMandatory</fullName>
        <active>true</active>
        <description>SFDC1-2209 General Agreement value cannot be blank when General Agreement Type is selected as 
SFDC1-5851 Changing error message as per requirement
SFDC1-7189 Updating error for Master Agreement</description>
        <errorConditionFormula>AND(
	(TEXT(GeneralAgreementType__c)=&quot;Other&quot;),
	ISBLANK( GeneralAgreementValue__c)
)</errorConditionFormula>
        <errorMessage>Please enter the Other Agreement Name  when Master Agreement Type is selected as &apos;Other&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GlobalAllianceFieldsMandatory</fullName>
        <active>true</active>
        <description>SFDC1-4421 Global Alliance fields are mandatory when Sales Channel on Opportunity is set to &apos;Global Alliance&apos;. SFDC1-5260- Editing this validation rule for only contact fields.
Modified formula as per bug fix SFDC1-6656.</description>
        <errorConditionFormula>AND( 
	(TEXT(Opportunity__r.SubType__c)=&apos;Global Alliance&apos;), 
	RecordType.Name = &apos;Order Form&apos;, 
	NOT(ISNEW()), 
	ByPassValidationsForProcessBuilder__c == FALSE, 
	NOT(ISCHANGED(ByPassValidationsForProcessBuilder__c)),
	NOT(ISCHANGED(TotalContractValue__c)),
	NOT(ISCHANGED(FirstYearContractValue__c)),
	OR( 
		ISBLANK( GlobalAllianceContactFirstName__c ), 
		ISBLANK( GlobalAllianceContactLastName__c ), 
		ISBLANK( GlobalAllianceContactEmailAddress__c ), 
		ISBLANK( GlobalAllianceContactPhoneNumber__c ) 
	) 
)</errorConditionFormula>
        <errorMessage>Please provide all  Global Alliance Contact Related information as the Opportunity associated to this Contract has &apos;Global Alliance&apos; Sales Channel.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockCompletedContracts</fullName>
        <active>true</active>
        <description>SFDC1-4687 : Contract Records are locked for editing after stage is completed.SFDC1-6070 : Added Order management Profile. SFDC1-5918:To allow the contract status and Replacement contract fields to get updated. 
SFDC1-6970: By passing validation rule</description>
        <errorConditionFormula>AND(
IF(NOT(ISCHANGED(Status)) &amp;&amp; 
NOT(ISCHANGED(ContractStatus__c)) &amp;&amp; NOT(ISCHANGED(ReplacementContract__c)) &amp;&amp; ISPICKVAL(Status, &quot;Complete&quot;) ,true,false), 
ByPassValidationsForProcessBuilder__c == FALSE, 
NOT(ISCHANGED(ByPassValidationsForProcessBuilder__c)),
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;, 
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin: API&quot;,
NOT(CONTAINS($Profile.Name, &quot;Order Management&quot;)),
NOT(CONTAINS($Profile.Name, &quot;Contract Management&quot;)),
NOT(CONTAINS($Profile.Name, &quot;Sales Ops&quot;))
)</errorConditionFormula>
        <errorMessage>Contract Records are locked for editing after stage is completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MarkitGeneralAgreementFieldMandatory</fullName>
        <active>false</active>
        <description>SFDC1-2209  This rule makes Markit General Agreement field mandatory if Type picklist value equals to Markit General Agreement.
SFDC1-5851 Deactivating validation rule as per requirement.</description>
        <errorConditionFormula>AND(
	(TEXT(GeneralAgreementType__c) = &quot;Markit General Agreement&quot;), 
	(ISBLANK(MarkitGeneralAgreement__c)),
	NOT(CONTAINS($Profile.Name,&quot;Admin&quot;))
)</errorConditionFormula>
        <errorMessage>Please select Markit General Agreement when Master Agreement type is &apos;Markit General Agreement&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoInternalApprovalNeeded</fullName>
        <active>true</active>
        <description>SFDC1-6282: No Internal Approval is needed when status is Generation, else needed.SFDC1-7774: Prevented this validation on Order Form with Compliance restriction.SFDC1-8355 Update logic to use the new Opportunity&apos;s Compliance Restrictions field.Added 8822</description>
        <errorConditionFormula>AND( 
    OR(RecordType.Name &lt;&gt; &apos;Order Form&apos;,
       AND(RecordType.Name == &apos;Order Form&apos;, 
           Opportunity__r.ComplianceRestrictions__c == &apos;&apos;
          )
       ),
       AND( 
          OR(ISPICKVAL(Status, &quot;Generation&quot;),
	         ISPICKVAL(Status, &quot;ReadyForProcessing&quot;)),
             ISPICKVAL(PRIORVALUE(Status), &quot;Creation&quot;)		   
       ),
       OR(
           AND(
                 NoInternalApprovalNeeded__c  = false,
                 NumApprovalsApproved__c &gt; 0,
		 NumApprovalRequested__c &gt; 0,
                 NOT(NumApprovalsApproved__c = NumApprovalRequested__c)
		 
	   ),
           AND(
                 NoInternalApprovalNeeded__c  = false,
                 NumApprovalsApproved__c = NumApprovalRequested__c,
                 NumApprovalsApproved__c = 0,
		 NumApprovalRequested__c = 0
                 
		 
	   )
         )      
)</errorConditionFormula>
        <errorDisplayField>NoInternalApprovalNeeded__c</errorDisplayField>
        <errorMessage>If moving Contract Stage to Generation or Ready For Processing, please indicate that no internal approvals are needed for this contract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OaRFieldsForReadyForProcessingContract</fullName>
        <active>true</active>
        <description>SFDC1-6709: We should not allow the Opportunity to be closed Won until the user updates the status on the Opportunity at Risk record to Closed when Auto Renewal is False</description>
        <errorConditionFormula>AND(
Opportunity__r.OpportunityRecordType__c = &apos;Renewal&apos;, Opportunity__r.OaRStatus__c = &apos;Open - Pending Resolution&apos;,
TEXT(Status)=&apos;ReadyforProcessing&apos;,
Opportunity__r.AutoRenewal__c = FALSE
)</errorConditionFormula>
        <errorMessage>Please update the status on the Opportunity at Risk record before setting Contract to Ready for Processing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityCheckOnContractStatusRejected</fullName>
        <active>true</active>
        <description>SFDC1-5673 : When Contract status is Rejected, the Opportunity is set to stage 5, So to prevent Process builder failure, checking Opportunity Mandatory fields on stage 5.</description>
        <errorConditionFormula>AND( 
	TEXT(ContractStatus__c) = &quot;Rejected&quot;,
	NOT(ISBLANK(Opportunity__c)),
	Opportunity__r.RecordType.Name = &quot;New Business&quot;,
	OR(
		ISBLANK(Opportunity__r.ContractStartDate__c),
		ISBLANK(Opportunity__r.ContractEndDate__c)
	)
)</errorConditionFormula>
        <errorMessage>Associated Opportunity must have Contract Dates on it before you Reject the Contract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpptyFieldsForReadyForProcessingContract</fullName>
        <active>true</active>
        <description>SFDC1-5691 In order to set Contract to &apos;Ready For Processing&apos; stage, certain Opportunity fields should be populated on &apos;New Business Opportunity&apos;.</description>
        <errorConditionFormula>AND(RecordType.Name =&apos;Order Form&apos;,
TEXT(Status)=&apos;ReadyforProcessing&apos;,
Opportunity__r.RecordType.Name == &apos;New Business&apos;,
OR (
ISBLANK(Opportunity__r.CloseDate),
Opportunity__r.ActivityCreated__c == FALSE,
ISBLANK(TEXT(Opportunity__r.BudgetRange__c))  
)
)</errorConditionFormula>
        <errorMessage>Please populate Budget range, Closed Date and Create an activity on the related Opportunity record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>POFieldsforReadyforProcessingStage</fullName>
        <active>false</active>
        <description>SFDC1-5253 PO Fields should be required when Contract status is Ready for Processing and PO Required is true

SFDC1-7190 - Remove PO required validation</description>
        <errorConditionFormula>AND(
RecordType.Name =&apos;Order Form&apos;, 
TEXT(Status)=&apos;ReadyforProcessing&apos;, 
PORequired__c=True,
OR(
ISBLANK(PO__c ),
ISBLANK(PODate__c) 
)
)</errorConditionFormula>
        <errorMessage>PO # and PO Date is required when &apos;PO Required&apos; is True and Stage is set to &apos;Ready for Processing&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventContractOwnerChange</fullName>
        <active>true</active>
        <description>SFDC1-4739 : Prevent Contract Owner Change by Specific profiles</description>
        <errorConditionFormula>AND(
ISCHANGED(OwnerId), 
OR(
CONTAINS($Profile.Name, &apos;Read Only Profile&apos;),
CONTAINS($Profile.Name, &apos;Customer Care&apos;),
CONTAINS($Profile.Name, &apos;IHSMarkit Delivery&apos;),
CONTAINS($Profile.Name, &apos;Order Management&apos;),
CONTAINS($Profile.Name, &apos;Data Governance&apos;),
CONTAINS($Profile.Name, &apos;Legal&apos;)
)  
)</errorConditionFormula>
        <errorMessage>You are not authorized to Change Contract Owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PriceIncreaseClauseRequiredForOrderForm</fullName>
        <active>true</active>
        <description>SFDC1-5201 Price Increase Clause Required when stage = ready for processing</description>
        <errorConditionFormula>AND (
 RecordType.Name == &apos;Order Form&apos;,
 TEXT(Status)=&apos;ReadyforProcessing&apos;,
 ISBLANK(TEXT(PriceIncreaseClause__c))
)</errorConditionFormula>
        <errorDisplayField>PriceIncreaseClause__c</errorDisplayField>
        <errorMessage>&apos;Price Increase Clause&apos; is required for order form when stage is &apos;Ready for Processing&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReadyForProcessingValidation</fullName>
        <active>true</active>
        <description>SFDC1-7745: For all record types except for Continuation Letters, the Contract Status must equal &quot;Signed By Customer&quot; to move the Contract Stage to Ready for Processing
SFDC1-8205: Updated this validation rule to fire only when status is changed</description>
        <errorConditionFormula>AND( 
RecordType.DeveloperName &lt;&gt; &quot;ContinuationLetter&quot;, 
ISCHANGED(Status),
NOT(ISPICKVAL(  ContractStatus__c  , &quot;Signed By Customer&quot;)), NOT(ISPICKVAL(  ContractStatus__c , &quot;Rejected&quot;)), 
ISPICKVAL( Status , &quot;ReadyForProcessing&quot;))</errorConditionFormula>
        <errorMessage>Client Signature is required before the Contract can be moved to Ready for Processing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RenewalOpptyFieldsForReadyForProcessing</fullName>
        <active>true</active>
        <description>SFDC1-5691 In order to set Contract to &apos;Ready For Processing&apos; stage, certain Opportunity fields should be populated on &apos;Renewal Opportunity&apos;.</description>
        <errorConditionFormula>AND(RecordType.Name =&apos;Order Form&apos;,
TEXT(Status)=&apos;ReadyforProcessing&apos;,
Opportunity__r.RecordType.Name == &apos;Renewal&apos;,
OR (
ISBLANK(Opportunity__r.CloseDate),
Opportunity__r.ActivityCreated__c == FALSE
)
)</errorConditionFormula>
        <errorMessage>Please populate Closed Date and Create an activity on the related Opportunity record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequiredFieldsforReadyforProcessingStage</fullName>
        <active>true</active>
        <description>SFDC1-5253 Required fields for setting Contract in &apos;Ready for Processing&apos; Status: SFDC1-6993 Removing instance of Invoice Hold as it is no more required to be a check for Contract Status as per Business SFDC1-8205: Updated this VR to fire on status update</description>
        <errorConditionFormula>AND(RecordType.Name =&apos;Order Form&apos;,
ISCHANGED(Status), 
TEXT(Status)=&apos;ReadyforProcessing&apos;,
OR(
ISBLANK(TEXT(InvoiceFormat__c)),
ISBLANK(TEXT(PaymentTerms__c))
)
)</errorConditionFormula>
        <errorMessage>Invoice Format and Payment terms are required to change Contract Stage to &apos;Ready for Processing&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WhenUsageReportsRequiredIsTrue</fullName>
        <active>true</active>
        <description>SFDC1-9301 : New fields added to contract Orderform</description>
        <errorConditionFormula>AND (
RecordType.Name == &apos;Order Form&apos;,
ISPICKVAL(UsageReportsRequired__c,&apos;Yes&apos;),
ISBLANK(NameProvidedUsageDetails__c),
ISBLANK(EmailAddress__c)
)</errorConditionFormula>
        <errorMessage>If Usage Reports Required = Yes then ‘Name (provided usage details)’ and ‘Email Address’ fields are required</errorMessage>
    </validationRules>
