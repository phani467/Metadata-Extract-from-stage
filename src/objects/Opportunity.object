&amp;&amp; NOT CONTAINS($Profile.Name , &apos;System Admin&apos;)
&amp;&amp; (ByPassValidation__c == false)</errorConditionFormula>
        <errorMessage>Cannot delete Opportunity products, please change the opportunity product&apos;s status to &quot;Lost&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReadyForProcessingContractExist</fullName>
        <active>true</active>
