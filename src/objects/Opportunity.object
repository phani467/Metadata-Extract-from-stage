)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Please note that you can no longer edit 6WP opportunity. If you need any assistance please contact Global Sales Operations</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage4MandatoryFields</fullName>
        <active>true</active>
        <description>SFDC1-547 : To confirm the mandatory fields once moved to Stage 4 on New Business
SFDC1-6313 : Added By Pass Validation rule field</description>
        <errorConditionFormula>AND( NOT(ByPassValidation__c),  
			TEXT(StageName) = &quot;4-Proposal Accepted and Verbal Commit&quot;,
			RecordType.Name = &quot;New Business&quot;,
		 NOT(ActivityCreated__c)
)</errorConditionFormula>
        <errorMessage>On Stage 4-Proposal Accepted and Verbal Commit, Opportunity must have an Activity referenced to it</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage5MandatoryFields</fullName>
        <active>true</active>
        <description>SFDC1-548 : To confirm the mandatory fields once moved to Stage 5 on New Business, SFDC1-5866 : removed Activity check on Stage 5.</description>
        <errorConditionFormula>AND( 
				TEXT(StageName) = &quot;5-Contracting&quot;,
			 RecordType.Name = &quot;New Business&quot;,
   OR(
						ISBLANK(ContractStartDate__c),
		ISBLANK(ContractEndDate__c)
				)
)</errorConditionFormula>
        <errorMessage>On Stage 5-Contracting Opportunity must have Contract Dates on it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StageUpdateTo6WPNotAllowed</fullName>
        <active>true</active>
        <description>SFDC1-7855: Only Order Management and Admin profiles can change stage to   
 &quot;6WP-Closed Won/Order Processed&quot;</description>
        <errorConditionFormula>AND(
   $Profile.Name &lt;&gt; &apos;IHSMarkit System Admin&apos;,
   $Profile.Name &lt;&gt; &apos;System Administrator&apos;,
   NOT(CONTAINS($Profile.Name,&apos;Order Management&apos;)),
   AND(
       ISCHANGED( StageName ),
       ISPICKVAL( StageName , &quot;6WP-Closed Won/Order Processed&quot;) 
   )
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Only Order Management can set the Sales Stage to &quot;6WP-Closed Won/Order Processed&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StoppingAutoRenewValidation</fullName>
        <active>true</active>
        <description>SFDC1-1845 When &apos;Auto Renew&apos; is unchecked and user clicks &apos;Stop Auto Renew&apos;, this validation should prevent that (since opportunity is not auto renew).</description>
        <errorConditionFormula>AND (
	AutoRenewal__c == False,
	DoesThisAutoRenewalHaveChanges__c == TRUE
	)</errorConditionFormula>
        <errorMessage>This Opportunity is not an Auto Renewal.</errorMessage>
    </validationRules>
