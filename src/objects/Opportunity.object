		height: 783,
		open: function(event, ui) {
		  if($g(&apos;#myNewImage&apos;).length == 0) {
		     $g(&quot;.ui-dialog-titlebar&quot;).append(&quot;&lt;img src=&apos;&quot;+getDnbLogo()+&quot;&apos; id=&apos;myNewImage&apos; /&gt;&quot;);
