        <errorConditionFormula>AND( 
OpportunityRecordType__c =&apos;Renewal&apos;,
(ISCHANGED(OwnerId)), 
NOT(
OR(
CONTAINS($Profile.Name,&quot;System Admin&quot;),
AND( 
	CONTAINS($Profile.Name, &apos;Sales Ops&apos;), 
	$Permission.ReassignRenewals
	)
)
)
