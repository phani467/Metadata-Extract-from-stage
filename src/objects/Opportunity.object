        <errorConditionFormula>NOT(ISPICKVAL( Account.DataGovernanceStatus__c , &apos;Approved&apos;))</errorConditionFormula>
        <errorMessage>Opportunities can only be created for or associated with accounts that have been &quot;Approved&quot; by Data Governance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reopening6LOpportunityforNewBusiness</fullName>
        <active>true</active>
        <description>SFDC1-304;A validation rule to include the criteria of reopening the opportunity in 30 days, else an error should be displayed.(Only for Order Management, Sales Ops, Opportunity Owner)</description>
        <errorConditionFormula>AND(
	ISPICKVAL(PRIORVALUE(StageName),&quot;6L-Closed Lost&quot;),
	$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
	OR($RecordType.Name = &quot;New Business&quot;,
	$RecordType.Name = &quot;Placeholder&quot;),
	(Today()- CloseDate&gt;30),
  	OR(NOT(CONTAINS($Profile.Name,&quot;Sales Ops&quot;)),
          $Profile.Name &lt;&gt; &quot;IHSMarkit Order Management&quot;, 
                OwnerId &lt;&gt; $User.Id,
         NOT(CONTAINS($Profile.Name,&quot;Sales Ops&quot;))))</errorConditionFormula>
        <errorMessage>Please note that you can no longer edit closed Lost Opportunity after 30 days of closing.If you need any assistance please contact System Administration.</errorMessage>
    </validationRules>
    <validationRules>
