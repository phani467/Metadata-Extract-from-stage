OR( 
AND(IsClosed = False , ISPICKVAL( ForecastCategoryName , &quot;Pipeline&quot;) , CloseDate &lt; TODAY()+ 30), 
AND(IsClosed = False , CloseDate - CloseDateOriginal__c &gt; 60) 
),IMAGE(&quot;/img/samples/light_yellow.gif&quot;, &quot;Yellow&quot;), 
