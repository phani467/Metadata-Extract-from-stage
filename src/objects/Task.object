    <validationRules>
        <fullName>RestrictTaskCreateandEditbyProfiles</fullName>
        <active>true</active>
        <description>SFDC1-4739 : Restrict some Profiles from Task creation and edit</description>
        <errorConditionFormula>AND(
OR(
ISNEW(), 
ISCHANGED(Subject),
ISCHANGED(WhoId),
ISCHANGED(CurrencyIsoCode),
ISCHANGED(OwnerId),
ISCHANGED(Lead__c),
ISCHANGED(Description),
ISCHANGED(Priority),
ISCHANGED(Status),
ISCHANGED(ActivityDate),
ISCHANGED(WhatId),
ISCHANGED(Type)
), 
OR(
CONTAINS($Profile.Name, &apos;Read Only Profile&apos;),
CONTAINS($Profile.Name, &apos;Data Governance&apos;),
CONTAINS($Profile.Name, &apos;Sales: Product Manager&apos;),
CONTAINS($Profile.Name, &apos;Sales: Marketing&apos;)
),
LEFT(WhatId, 3) == &apos;800&apos;
)</errorConditionFormula>
        <errorMessage>You are not authorized to create or edit Tasks.</errorMessage>
    </validationRules>
</CustomObject>
