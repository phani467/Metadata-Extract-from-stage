IF(NOT(ISBLANK(Street__c)) || NOT(ISBLANK(City__c)) || NOT(ISBLANK(StateProvince__c)) || NOT(ISBLANK(ZipPostalCode__c)), BR(), &apos;&apos;) + 
IF(NOT(ISBLANK(Country__c)), Country__c, &apos;&apos;)</formula>
