        <linkType>javascript</linkType>
        <masterLabel>Send to Opportunity Team</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>if(&apos;{!MeetingNote__c.NumberOfOpportunities__c}&apos; == 0) { 
alert(&apos;Please link an Opportunity to the meeting note to send to Opportunity Team&apos;); 
} else { 
