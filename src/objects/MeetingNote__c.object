        <masterLabel>Send to Account Team</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>if(&quot;{!MeetingNote__c.AccountId__c}&quot; == null || &quot;{!MeetingNote__c.AccountId__c}&quot; == &quot;&quot;) { 
alert(&apos;Please link an Account to the meeting note to send to Account Team&apos;); 
} else { 
var url = &quot;/apex/SendMeetingNoteToAccountTeam?meetingId={!MeetingNote__c.Id}&quot; 
window.location.href = url; 
}</url>
    </webLinks>
    <webLinks>
