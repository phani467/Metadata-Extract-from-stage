        <active>true</active>
        <description>SFDC1-4739 : Restrict some Profiles from Attachment creation and edit</description>
        <errorConditionFormula>AND( 
OR( 
ISNEW(), 
ISCHANGED(AccountName__c), 
ISCHANGED(AttachmentType__c), 
ISCHANGED(Comments__c), 
ISCHANGED(Contract__c), 
ISCHANGED(ContractEndDate__c), 
ISCHANGED(ContractStartDate__c), 
ISCHANGED(DocuSignEnvelopeId__c), 
ISCHANGED(IsDocuSignCert__c), 
ISCHANGED(Name)
), 
OR( 
CONTAINS($Profile.Name, &apos;Read Only Profile&apos;), 
CONTAINS($Profile.Name, &apos;Data Governance&apos;), 
CONTAINS($Profile.Name, &apos;Product Manager&apos;), 
CONTAINS($Profile.Name, &apos;Marketing&apos;),
CONTAINS($Profile.Name, &apos;Read Only Activity User&apos;) 
) 
)</errorConditionFormula>
        <errorMessage>You are not authorized to add Attachments.</errorMessage>
    </validationRules>
</CustomObject>
