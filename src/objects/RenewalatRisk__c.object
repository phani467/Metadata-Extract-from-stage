    <validationRules>
        <fullName>Check_if_Renewal_at_Risk_already_exists</fullName>
        <active>true</active>
        <description>SFDC1-4160 One opportunity is allowed to have only one renewal at risk associated to it.</description>
        <errorConditionFormula>AND((OpportunityAtRisk__r.Countofrenewalatrisk__c = 1),ISNEW())</errorConditionFormula>
        <errorMessage>This Opportunity has already been flagged for being &apos;at Risk&apos;.</errorMessage>
    </validationRules>
