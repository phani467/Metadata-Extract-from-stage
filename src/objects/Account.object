    <validationRules>
        <fullName>AccountSubTypeValueRequired</fullName>
        <active>true</active>
        <description>SFDC1-4380 if &quot;Global Alliance&quot; is selected in the &quot;Account Type&quot; field the user is required to select a value from the &quot;Account Sub Type&quot; picklist.</description>
        <errorConditionFormula>ISPICKVAL(Type, &apos;Global Alliance&apos;)
&amp;&amp; ISBLANK(AccountSubType__c)</errorConditionFormula>
        <errorDisplayField>AccountSubType__c</errorDisplayField>
        <errorMessage>Account Sub Type is required if Account Type is &quot;Global Alliance&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillingAddressIsRequired</fullName>
        <active>true</active>
        <description>SFDC1-2235 validate Account&apos;s Billing Address and pop up error if it&apos;s missing</description>
        <errorConditionFormula>ISBLANK(BillingStreet)
|| ISBLANK(BillingCity)
|| ISBLANK(BillingCountry)</errorConditionFormula>
        <errorMessage>Account&apos;s address fields (Street, City and Country) are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BlockCreatingAccount</fullName>
        <active>true</active>
        <description>SFDC1-684 Restrict the creation of Accounts to System Admin and Data Governance.</description>
        <errorConditionFormula>ISNEW()
&amp;&amp; $Profile.Name &lt;&gt; &quot;IHSMarkit Data Governance&quot;
&amp;&amp; $Profile.Name &lt;&gt; &quot;System Administrator&quot;</errorConditionFormula>
        <errorMessage>Only Data Governance or System Administrator can create account.Please either select an existing account or request one from the Lead page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BlockNameUpdateForNonDataGovernance</fullName>
        <active>true</active>
        <description>SFDC1-2056 &amp; SFDC1-7981 Account Name and Status field should be locked down for all account, only Data Governance and System Admin should be able to edit these field.</description>
        <errorConditionFormula>(ISCHANGED(Name) || ISCHANGED(Status__c))
&amp;&amp; $Profile.Name &lt;&gt; &quot;IHSMarkit Data Governance&quot; 
&amp;&amp; $Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;
&amp;&amp; $Profile.Name &lt;&gt; &quot;System Administrator&quot;</errorConditionFormula>
        <errorMessage>Only Data Governance and System Admin can edit Account Name and Status fields.Please either select an existing account or request one from the Lead page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BlockProhibitedCountry</fullName>
        <active>true</active>
        <description>SFDC1-383 Do not allow an Account record to be created if the Account&apos;s country is a Prohibited Country</description>
        <errorConditionFormula>VLOOKUP( $ObjectType.Country__c.Fields.IsProhibited__c ,  $ObjectType.Country__c.Fields.Name ,  BillingCountry )</errorConditionFormula>
        <errorDisplayField>BillingCountry</errorDisplayField>
        <errorMessage>Due to U.S. export controls and sanctions restrictions, Account records cannot be created for the selected country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DataGovernanceStatusApproved</fullName>
        <active>true</active>
        <description>SFDC1-2259 make sure Data Governance&apos;s required fields are populated before Data Governance Status could be changed to &apos;Approved&apos;.
SFDC1-8169 Website is no longer required because this data is not always provided.</description>
        <errorConditionFormula>ISPICKVAL(DataGovernanceStatus__c, &quot;Approved&quot;)
&amp;&amp;
(
   ISPICKVAL(Type, &quot;&quot;)
|| ISPICKVAL(Priority__c, &quot;&quot;)
)</errorConditionFormula>
        <errorMessage>Account Type and Priority are required for Data Governance Status to be set to &quot;Approved&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DataGovernanceStatusApprovedInactive</fullName>
        <active>true</active>
        <description>SFDC1-5259 prevents an account from having Data Governance Status of Approved and Account Status of &quot;Invalid Inactive&quot;</description>
        <errorConditionFormula>ISPICKVAL(DataGovernanceStatus__c, &quot;Approved&quot;)
&amp;&amp; ISPICKVAL(Status__c, &quot;InactiveInvalid&quot;)</errorConditionFormula>
        <errorMessage>Account record cannot have Data Governance Status of &quot;Approved&quot; if the Status is &quot;Inactive - Invalid&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExternalOrgTypeRequired</fullName>
        <active>true</active>
        <description>SFDC1-797 External Org Type is required if Institution Type is selected</description>
        <errorConditionFormula>ISPICKVAL(InstitutionType__c, &apos;&apos;) = false
&amp;&amp; ISPICKVAL(ExternalOrgType__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>ExternalOrgType__c</errorDisplayField>
        <errorMessage>External Org Type is required if Institution Type is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StateOfIncorporationIsRequiredForUS</fullName>
        <active>true</active>
        <description>SFDC1-4360 if &quot;United States&quot; is selected in the &quot;Incorporated In&quot; field then &quot;State of Incorporation&quot; is mandatory.</description>
        <errorConditionFormula>IncorporatedIn__r.CountryCode__c = &apos;US&apos;
&amp;&amp; ISBLANK(TEXT(StateOfIncorporation__c))</errorConditionFormula>
        <errorDisplayField>IncorporatedIn__c</errorDisplayField>
        <errorMessage>If &quot;Incorporated In&quot; country is &quot;United States&quot;, please select &quot;State of Incorporation&quot; as it is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidatedAccountFields</fullName>
        <active>true</active>
        <description>Once validated (DataGovernanceStatus = &quot;Approved&quot;), all account fields except &quot;Description&quot; and &quot;Formerly Known As&quot; are only editable by Data Governance or System Administrator.</description>
        <errorConditionFormula>ISPICKVAL(DataGovernanceStatus__c, &quot;Approved&quot;)
&amp;&amp; $Profile.Name &lt;&gt; &quot;IHSMarkit Data Governance&quot; 
&amp;&amp; $Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;
&amp;&amp; $Profile.Name &lt;&gt; &quot;System Administrator&quot;
&amp;&amp; 
(ISCHANGED(AlternateNameTradingName__c)
|| ISCHANGED(Status__c)
|| ISCHANGED(Phone)
|| ISCHANGED(Website)
|| ISCHANGED(CurrencyIsoCode)
|| ISCHANGED(VATTaxID__c)
|| ISCHANGED(BillingStreet)
|| ISCHANGED(BillingCity)
|| ISCHANGED(BillingState)
|| ISCHANGED(BillingCountry)
|| ISCHANGED(BillingPostalCode)
)</errorConditionFormula>
        <errorMessage>Users may update the Account Description and Formally Known As fields. Please use *Account Assistance* to open a case for Data Governance to request any other changes to this account.</errorMessage>
    </validationRules>
