                <apex:selectList label="Motorcycle 10" value="{!MC.MC10}" size="1">
                    <apex:selectOptions value="{!MC.MOTO}" />
                </apex:selectList>
            </apex:pageblockSection>

            <apex:pageBlockSection title="RV Data - Select as many fields as required"
