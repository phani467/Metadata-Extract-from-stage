<apex:component >

    <!-- SHOW THIS DATA SPECS SECTION IF Aftermarket WRF IS SELECTED -->
    <apex:attribute type="WRACE" description="extension of parent page"
        name="pageExt" required="true" />
    <apex:attribute name="wrf"
        description="This is the active Work Request Form" required="true"
        type="WorkRequestForm__c" />
    <apex:attribute name="mc"
        description="This is the active Motorcycle Display set"
        required="true" type="MotorCycle" />
    <apex:attribute name="cnt"
        description="This is the active Cars and Trucks Fields set"
        required="true" type="CarsNTrucks" />
    <apex:actionRegion >


        <!-- *********** START REGULAR PRINT SECTION ****************************************************************************************************************** -->

        <!-- ALWAYS SHOW THIS GENERAL SECTION -->

        <apex:pageblock title="Parc Contracts"
            rendered="{!wrf.PARCDataFiles__c}">
            <apex:outputText >Deprecated Parc system contract entry. </apex:outputText>
            <apex:outputField value="{!wrf.DeprecatedParcSystem__c}" />
        </apex:pageblock>

        <apex:pageblock title="Common Selections"
            rendered="{!(wrf.ReportDataFile__c||wrf.PARCDataFiles__c||wrf.WorldviewVIO__c||wrf.WorldviewParc__c||wrf.WorldviewGAMS__c)&&(ISBLANK(wrf.SubmittedDate__c) || wrf.SubmittedDate__c > DATE(2014,06,16))}">

            <apex:pageblocksection title="Vehicle" columns="2"
                collapsible="false"
                rendered="{!IF((wrf.ReportDataFile__c||wrf.WorldviewVIO__c) && (ISBLANK(wrf.SubmittedDate__c) || wrf.SubmittedDate__c > Date(2015,07,24)),FALSE,TRUE)}">
                <apex:outputField label="VIN GVW/Category" value="{!wrf.GVW__c}"
