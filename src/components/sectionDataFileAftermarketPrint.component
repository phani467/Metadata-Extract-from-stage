                    <apex:pageblocksectionitem >
                        <apex:outputlabel for="us" value="US" styleclass="requiredField" />
                        <apex:outputField value="{!wrf.US__c}" />
                    </apex:pageblocksectionitem>
                    <apex:pageblocksectionitem >
                        <apex:outputlabel for="can" value="Canada 7/1 Only"
