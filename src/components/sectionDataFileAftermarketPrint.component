                    rendered="{!wrf.ReportDataFile__c||wrf.WorldviewVIO__c||wrf.WorldviewParc__c||wrf.WorldviewGAMS__c ||wrf.PARCDataFiles__c}" />
                <apex:outputField value="{!wrf.OEEngineAttributes__c}"
                    rendered="{!wrf.WorldviewParc__c||wrf.WorldviewGAMS__c}" />
                <apex:outputField value="{!wrf.OEGearboxAttributes__c}"
                    rendered="{!wrf.WorldviewParc__c||wrf.WorldviewGAMS__c}" />
                <apex:outputField value="{!wrf.OEVehicleAttributes__c}"
                    rendered="{!wrf.WorldviewParc__c||wrf.WorldviewGAMS__c}" />

            </apex:pageblocksection>

            <apex:pageBlockSection title="Time Periods" id="time" columns="2"
                collapsible="false"
                rendered="{!wrf.WorldviewParc__c||wrf.WorldviewGAMS__c||wrf.ReportDataFile__c||wrf.WorldviewVIO__c||(wrf.PARCDataFiles__c && ( ISBLANK(wrf.SubmittedDate__c) || wrf.SubmittedDate__c > Date(2015,07,24)))}">

                <apex:outputField value="{!wrf.TimePeriodsAftermarket__c}"
                    styleclass="requiredField" />


                <apex:outputField label="Specify specific years or year range"
                    value="{!wrf.TimePeriodDescription__c}"
                    styleclass="requiredField" style="width: 300px; height: 100px;" />

            </apex:pageBlockSection>

            <apex:pageBlockSection title="Other Requirements" columns="2"
                collapsible="false"
                rendered="{!(wrf.ReportDataFile__c && !ISBLANK(wrf.SubmittedDate__c) && wrf.SubmittedDate__c < Date(2015,07,24))||wrf.WorldviewVIO__c||wrf.WorldviewGAMS__c||wrf.WorldviewParc__c||(wrf.PARCDataFiles__c && (ISBLANK(wrf.SubmittedDate__c) || wrf.SubmittedDate__c > Date(2015,07,24)))}">

