                        <apex:pageBlockSectionItem >
                            <apex:outputLabel for="cx41" value="4/1"
                                styleclass="requiredField" />
                            <apex:outputField value="{!wrf.X41__c}" id="cx41" />
                        </apex:pageBlockSectionItem>
                        <apex:outputField value="{!wrf.Year41__c}"
                            rendered="{!wrf.X41__c}" styleClass="requiredField" />
                        <apex:pageblocksectionitem rendered="{!!wrf.X41__c}" />

                        <apex:pageBlockSectionItem >
                            <apex:outputLabel for="cx71" value="7/1"
                                styleclass="requiredField" />
                            <apex:outputField value="{!wrf.X71__c}" id="cx71" />
                        </apex:pageBlockSectionItem>
                        <apex:outputField value="{!wrf.Year71__c}"
                            rendered="{!wrf.X71__c}" styleClass="requiredField" />
                        <apex:pageblocksectionitem rendered="{!!wrf.X71__c}" />

                        <apex:pageBlockSectionItem >
                            <apex:outputLabel for="cx101" value="10/1"
                                styleclass="requiredField" />
                            <apex:outputField value="{!wrf.X101__c}" id="cx101" />
                        </apex:pageBlockSectionItem>
                        <apex:outputField value="{!wrf.Year101__c}"
                            rendered="{!wrf.X101__c}" styleClass="requiredField" />
                        <apex:pageblocksectionitem rendered="{!!wrf.X101__c}" />
                        <apex:outputField style="width:100%"
                            value="{!wrf.OtherTimePeriods__c}" />
                </apex:pageBlockSection>

                <apex:pageblocksection rendered="{! !ISBLANK(wrf.SubmittedDate__c) && wrf.SubmittedDate__c < DATE(2014,06,17)}"
