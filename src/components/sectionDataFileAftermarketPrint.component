                <apex:selectList label="Motorcycle 1" value="{!MC.MC1}" size="1">
                    <apex:selectOptions value="{!MC.MOTO}" />
                </apex:selectList>
                <apex:selectList label="Motorcycle 6" value="{!MC.MC6}" size="1">
                    <apex:selectOptions value="{!MC.MOTO}" />
                </apex:selectList>
                <apex:selectList label="Motorcycle 2" value="{!MC.MC2}" size="1">
