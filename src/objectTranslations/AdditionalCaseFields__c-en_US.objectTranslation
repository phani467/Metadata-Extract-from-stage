        <errorMessage><!-- Please Note: You cannot associate more than one product to the case. --></errorMessage>
        <name>AllowOnlyOneProductPerCase</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When audit is true then audit comments should be populated --></errorMessage>
        <name>AuditCommentsIsMandatoryWhenAuditIsTrue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When audit is true then audit date field should be populated --></errorMessage>
        <name>AuditDateIsMandatoryWhenAuditIsTrue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When audit is true then audit rating should be populated --></errorMessage>
        <name>AuditRatingIsMandatoryWhenAuditIsTrue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When audit is true then auditor field should be populated --></errorMessage>
        <name>AuditorIsMandatoryWhenAuditIsTrue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please input on Customer Platform. --></errorMessage>
        <name>CustomerPlatformRequired</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please input Incident Report Owner. --></errorMessage>
        <name>IncidentReportOwnerRequired</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If MIS case == &quot;TRUE&quot; then fields on MIS section to be required/mandatory --></errorMessage>
        <name>MISMandatoryRule</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When Issue Area is selected as Clearing then CCP Vendor field should be populated --></errorMessage>
        <name>MandatoryCCPVendorForIssueAreaValue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When component is selected as DMS then DMS should be populated --></errorMessage>
        <name>MandatoryDMSForComponentValue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If User List Request is selected, then Delivery Team should be &quot;Delivery Technical&quot; and case OMD status should be &quot;Re-Assigned&quot;. --></errorMessage>
        <name>MandatoryDeliveryTeamAndCaseOMDStatus</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Doc # and SDO # are required --></errorMessage>
        <name>MandatoryDocAndSDO</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Jusristicion should have value, if product module is Auctions. --></errorMessage>
        <name>MandatoryJuristictionForAuctionProduct</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When Issue Area is selected as MSERV Regulatory Reporting then Regulatory Reporting Regime field should be populated --></errorMessage>
        <name>MandatoryRegRepRegimeForIssueAreaValue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When component is selected as SEF/MTF/OTF then SEF should be populated --></errorMessage>
        <name>MandatorySEFForComponentValue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Case Team Select should be set to &quot;Delivery&quot; when  Case Product Group is Delivery Helpdesk. --></errorMessage>
        <name>MandatoryTeamSelectAsDelivery</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Case Team Select should be  set to &quot;Order Management&quot; when Case Product Group is  &quot;Order Management Helpdesk&quot; --></errorMessage>
        <name>MandatoryTeamSelectIFCaseProdGroupOMH</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please input Customer Platform. --></errorMessage>
        <name>RequiredCustomerPlatformWithProductDataS</name>
    </validationRules>
