<aura:component controller="SubmitToSalesProcessController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global" >
	<aura:attribute name="recordId" type="Id" />
    <aura:handler name="init" action="{!c.doInit}" value="{!this}" />
    <div id="alertSuccess" class="" style="display:none">    
        	<h1 id="header1" class="slds-text-heading--medium" style="text-align: center;" >Successfully Submitted</h1>
        </div>
    	<div id="alertProcessing" class="" style="display:block">    
        	<h1 id="header1" class="slds-text-heading--medium" style="text-align: center;">Submitting to Sales Processing...</h1>
        </div>
</aura:component>